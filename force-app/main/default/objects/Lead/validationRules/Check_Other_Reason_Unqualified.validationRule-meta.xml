<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Other_Reason_Unqualified</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
   ISPICKVAL(Close_Lead_Reason__c , &quot;Other&quot;),
   LEN( Other_Reason__c ) &lt; 1
)</errorConditionFormula>
    <errorMessage>User have to fill in field Other Reason!</errorMessage>
</ValidationRule>
