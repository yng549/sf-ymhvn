<apex:page
    applyHtmlTag="false"
    showHeader="false"
    sidebar="false"
    standardStylesheets="false"
    applyBodyTag="false"
    contentType="text/html; charset=UTF-8"
    renderAs="pdf"
    controller="SaleOrderPCAPDFController"
>
    <html>
        <head>
            <style type="text/css" media="print">
                @page {
                    /* counter-increment: page; */
                    size: A4 portrait;
                    margin-top: 115px;
                    margin-left: 60px;
                    margin-right: 60px;
                    margin-bottom: 20px;
                    page-break-inside: avoid;
                    tr {
                        page-break-inside: avoid !important;
                    }

                    td {
                        page-break-inside: avoid !important;
                    }
                    @top-center {
                        content: element(header);
                    }

                    @bottom-right {
                        content: element(footer);
                    }
                }

                *,
                *::after,
                *::before {
                    margin: 0;
                    padding: 0;
                    box-sizing: inherit;
                }

                .row {
                    width: 100%;
                    margin: 0 auto;
                }

                .row::after {
                    content: "";
                    display: block;
                    clear: both;
                }

                .col-1-of-2 {
                    float: left;
                    width: 50%;
                }

                .last-child {
                    margin-right: 0px;
                    margin-bottom: 0px;
                }

                b {
                    font-family: sans-serif;
                    font-weight: bold;
                }

                body {
                    font-family: Arial Unicode MS;
                    font-size: 9px;
                    box-sizing: border-box;
                }

                div.header {
                    display: block;
                    position: running(header);
                }

                .header-logo-box {
                    /* margin-left: 5px; */
                    margin-top: 20px;
                    margin-bottom: 5px;
                    width: 100%;
                }

                .header-logo-box::after {
                    content: "";
                    display: block;
                    clear: both;
                }

                .logo {
                    float: left;
                    margin-top: 3px;
                    width: 25%;
                }

                .header__subtext-box {
                    float: left;
                    margin-top: 2px;
                    /* margin-left: 15px; */
                    text-align: left;
                    width: 70%;
                    /* margin-left: 15px; */
                }

                .box {
                    min-height: 36px;
                    width: 100%;
                }

                div.footer {
                    position: running(footer);
                }

                .container {
                    width: 100%;
                    font-size: 80%;
                    text-align: right;
                }

                .header__small-text span {
                    display: block;
                    font-size: 9px;
                }

                .header__text-box {
                    letter-spacing: 8px;
                    text-align: center;
                }

                .heading-primary {
                    text-transform: uppercase;
                    font-weight: bold;
                    font-size: 20px;
                }

                .heading-secondary {
                    text-transform: uppercase;
                    font-size: 16px;
                    font-family: sans-serif;
                    font-weight: bold;
                    font-style: italic;
                }

                .header__info-box p {
                    margin-bottom: 5px;
                }

                table {
                    width: 100%;
                    height: 100%;
                    border-collapse: collapse;
                }

                th {
                    font-weight: bold;
                    /* font-size: 9px; */
                    /* background-color: #c0c0c0; */
                    color: red;
                    text-align: center;
                    vertical-align: center;
                }

                td {
                    text-align: left;
                    padding-top: 2px;
                    padding-bottom: 2px;
                    padding-left: 3px;
                    padding-right: 3px;
                    overflow-wrap: break-word;
                    vertical-align: top;
                    /* line-height: 10px; */
                    /* font-size: 8px; */
                }

                table,
                th,
                td {
                    border: 1px solid black;
                }

                .color-yellow {
                    width: 30%;
                    background-color: yellow;
                    color: red;
                    font-family: sans-serif;
                    text-decoration: underline;
                    text-decoration-color: red;
                }

                .long {
                    width: 19.04%;
                }

                .short {
                    width: 4.76%;
                }

                .medium {
                    width: 9.52%;
                }

                .fixed-half-width {
                    width: 50%;
                }

                .fixed-width {
                    width: 25%;
                }

                .table-info {
                    width: 95%;
                }

                .table-info1 {
                    width: 95%;
                    margin-left: 5%;
                }

                .table-info > span,
                .table-info1 > span {
                    color: red;
                }

                .signature > tr td {
                    text-align: center;
                }

                .last-child {
                    margin-right: 0px;
                    margin-bottom: 0px;
                }

                .u-height-medium {
                    height: 35px;
                }

                .u-height-big {
                    height: 65px;
                }

                .u-p-0 {
                    padding: 0;
                }

                .u-p-r-small {
                    padding-right: 4px;
                }

                .u-p-r-medium {
                    padding-right: 15px;
                }

                .u-m-t-xsmall {
                    margin-top: 8px;
                }

                .u-m-t-small {
                    margin-top: 15px;
                }

                .u-m-t-medium {
                    margin-top: 30px;
                }

                .u-m-t-large {
                    margin-top: 60px;
                }

                .u-m-b-medium {
                    margin-bottom: 20px;
                }

                .u-m-tb-small {
                    margin: 3px 0px;
                }

                .u-m-r-small {
                    padding-right: 20px;
                }

                .u-align-center {
                    text-align: center;
                }

                .u-align-left {
                    text-align: left;
                }

                .u-align-right {
                    text-align: right;
                }

                .u-width-99 {
                    width: 99.5%;
                }

                .u-m-0 {
                    margin: 0;
                }

                .repeatTableHeader {
                    -fs-table-paginate: paginate;
                }

                .repeatTableHeader,
                .repeatTableHeader > tr > td,
                .repeatTableHeader > tr > th {
                    border: none;
                    border-spacing: 0;
                }
            </style>
        </head>

        <body>
            <!-- Header -->
            <div class="header">
                <div class="header-logo-box">
                    <div class="logo">
                        <apex:image
                            id="logo"
                            value="{!$Resource[logo]}"
                            width="150"
                            height="30"
                        />
                    </div>
                    <div class="header__subtext-box">
                        <p class="header__small-text">
                            <span>
                                <b
                                    >{!objOrder.Account.DO_DL_Create_Account__r.Name}</b
                                >
                            </span>
                            <span
                                >Địa chỉ / Address:
                                {!objOrder.Account.DO_DL_Create_Account__r.Address__c}
                            </span>
                        </p>
                    </div>
                </div>
                <div class="header__text-box">
                    <p class="heading-primary">ĐƠN BÁN HÀNG</p>
                    <p class="heading-secondary">
                        SALE ORDER PART CLOTHING ACCESSORIES
                    </p>
                </div>
                <div class="row last-child">
                    <div class="col-1-of-2">
                        <div class="box">
                            <!-- <p>a text box </p> -->
                        </div>
                    </div>
                    <div class="col-1-of-2 last-child">
                        <div class="box">
                            <!-- <p>a text box </p> -->
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Header -->
            <!-- Footer -->
            <div class="footer">
                <div class="container">
                    <apex:outputText value="{0,date,dd'/'MM'/'yyyy}">
                        <apex:param value="{!today}" />
                    </apex:outputText>
                    - {!currentUserName}
                </div>
            </div>
            <!-- End footer -->

            <div class="section-content">
                <div class="row">
                    <!-- First container -->
                    <div class="col-1-of-2">
                        <div class="table-info">
                            <span>
                                THÔNG TIN KHÁCH HÀNG / CUSTOMER INFORMATION
                            </span>
                            <table>
                                <colgroup>
                                    <col class="fixed-half-width" />
                                    <col class="fixed-half-width" />
                                </colgroup>
                                <tr>
                                    <td>Mã Khách Hàng / Customer No.:</td>
                                    <td>{!objOrder.Account.AccountNumber}</td>
                                </tr>
                                <tr>
                                    <td>Tên Khách Hàng / Customer Name:</td>
                                    <td>{!objOrder.Account.Name}</td>
                                </tr>
                                <tr>
                                    <td class="u-height-medium">
                                        Địa chỉ / Address:
                                    </td>
                                    <td>{!objOrder.Account.Address_Full__c}</td>
                                </tr>
                                <tr>
                                    <td>Điện thoại / Phone:</td>
                                    <td>
                                        {!objOrder.Account.PersonMobilePhone}
                                    </td>
                                </tr>
                                <tr>
                                    <td>Địa chỉ Email/ Email address:</td>
                                    <td>{!objOrder.Account.PersonEmail}</td>
                                </tr>
                            </table>
                        </div>
                        <div class="table-info u-m-t-xsmall">
                            <span>
                                THÔNG TIN XUẤT HÓA ĐƠN / INVOICE INFORMATION
                            </span>
                            <table>
                                <colgroup>
                                    <col class="fixed-half-width" />
                                    <col class="fixed-half-width" />
                                </colgroup>
                                <tr>
                                    <td>Tên Khách Hàng / Customer Name:</td>
                                    <td>{!objOrder.Account.Name}</td>
                                </tr>
                                <tr>
                                    <td class="u-height-medium">
                                        Địa chỉ / Address:
                                    </td>
                                    <td>
                                        {!IF((objOrder.Address_Full__c!=null),objOrder.Address_Full__c,objOrder.Account.Address_Full__c)}
                                    </td>
                                </tr>
                                <tr>
                                    <td>Mã Số Thuế / Tax Code:</td>
                                    <td>
                                        {!IF((objOrder.Tax_Code__c !=null),
                                        objOrder.Tax_Code__c,objOrder.Account.Tax_Code__c)}
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                    <div class="col-1-of-2">
                        <div class="table-info1">
                            <span>
                                THÔNG TIN ĐƠN HÀNG / ORDER INFORMATION
                            </span>
                            <table>
                                <colgroup>
                                    <col class="fixed-half-width" />
                                    <col class="fixed-half-width" />
                                </colgroup>
                                <tr>
                                    <td>Số đơn hàng/Order No.:</td>
                                    <td>{!objOrder.Order_Number__c}</td>
                                </tr>
                                <tr>
                                    <td>Điểm Bán / Sale Point Code:</td>
                                    <td>{!acc.DO_DL_Code__c}</td>
                                </tr>
                                <tr>
                                    <td>Nhân Viên Bán Hàng / Sale Person:</td>
                                    <td>{!objOrder.Owner.Name}</td>
                                </tr>
                                <tr>
                                    <td>Phương thức bán / Sale Method:</td>
                                    <td>{!objOrder.Sale_Method__c}</td>
                                </tr>
                                <tr>
                                    <td>Ngày bán / Sales Date:</td>
                                    <td>
                                        <apex:outputText
                                            value="{0,Date,dd'/'MM'/'yyyy}"
                                        >
                                            <apex:param
                                                value="{!objOrder.EffectiveDate}"
                                            />
                                        </apex:outputText>
                                    </td>
                                </tr>
                                <tr>
                                    <td>Ngày giao / Delivery Date:</td>
                                    <td>
                                        <apex:outputText
                                            value="{0,Date,dd'/'MM'/'yyyy}"
                                        >
                                            <apex:param
                                                value="{!objOrder.PCA_Delivery_Date__c}"
                                            />
                                        </apex:outputText>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="u-height-medium">
                                        Địa chỉ giao hàng / Delivery Address:
                                    </td>
                                    <td>{!objOrder.Address_Full__c}</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="row u-m-t-small">
                    <div class="color-yellow u-align-center">
                        PART CLOTHING ACCESSORIES
                    </div>
                    <table class="u-m-t-xsmall">
                        <colgroup>
                            <col class="short" />
                            <col class="long" />
                            <col class="long" />
                            <col class="short" />
                            <col class="medium" />
                            <col class="medium" />
                            <col class="short" />
                            <col class="medium" />
                            <col class="medium" />
                            <col class="medium" />
                        </colgroup>
                        <tr>
                            <th rowspan="2">STT <br /> (No.)</th>
                            <th rowspan="2">
                                Mã Sản Phẩm <br />(Product Number)
                            </th>
                            <th rowspan="2">
                                Tên Sản Phẩm <br />(Description)
                            </th>
                            <th rowspan="2">ĐVT <br /> (Unit)</th>
                            <th rowspan="2">Số lượng <br />(Quantity)</th>
                            <th rowspan="2">
                                Đơn giá bao gồm thuế GTGT <br />(Unit price -
                                VAT)
                            </th>
                            <th rowspan="1" colspan="2">
                                Giảm Giá/Sản Phẩm (Discount)
                            </th>
                            <th rowspan="2">
                                Thành tiền sau giảm giá <br />(Amount after
                                discount)
                            </th>
                            <th rowspan="2">
                                Thành tiền bao gồm thuế GTGT <br />(Amount -
                                VAT)
                            </th>
                        </tr>
                        <tr>
                            <th rowspan="1" colspan="1">%</th>
                            <th rowspan="1" colspan="1">Số Tiền (Amount)</th>
                        </tr>
                        <apex:variable value="{!1}" var="count" />
                        <apex:repeat value="{!lstOrderItem}" var="product">
                            <tr>
                                <td class="u-align-center">{!FLOOR(count)}</td>
                                <td>{!product.Product_Code__c}</td>
                                <td>
                                    {!product.Product2.Name}
                                    <br /> {!product.Product2.PartsNameEng__c}
                                </td>
                                <td class="u-align-center">Cái</td>
                                <td class="u-align-center">
                                    <apex:outputText value="{0,number,0}">
                                        <apex:param
                                            value="{!product.Quantity}"
                                        />
                                    </apex:outputText>
                                </td>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!product.ListPrice}"
                                        />
                                    </apex:outputText>
                                </td>
                                <td class="u-align-center">
                                    <apex:outputText value="{0,number,0}">
                                        <apex:param
                                            value="{!((product.ListPrice-product.UnitPrice)/IF(product.ListPrice!=0,product.ListPrice,1))*100}"
                                        />
                                    </apex:outputText>
                                </td>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!(product.ListPrice-product.UnitPrice)}"
                                        />
                                    </apex:outputText>
                                </td>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!(product.ListPrice*product.Quantity)-((product.ListPrice-product.UnitPrice)*product.Quantity)}"
                                        />
                                    </apex:outputText>
                                </td>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!product.ListPrice*product.Quantity}"
                                        />
                                    </apex:outputText>
                                </td>
                            </tr>
                            <apex:variable var="count" value="{!count+ 1}" />
                        </apex:repeat>
                        <tr>
                            <th
                                colspan="9"
                                style="background-color: rgb(218, 218, 218)"
                            >
                                Tổng Số Tiền (Total Sale Amount)
                            </th>
                            <td class="u-align-right">
                                <apex:outputText value="{0,number,###,###}">
                                    <apex:param value="{!sumListPrice}" />
                                </apex:outputText>
                            </td>
                        </tr>
                        <tr>
                            <th
                                colspan="9"
                                style="background-color: rgb(218, 218, 218)"
                            >
                                Tổng Giá Trị Chiết Khấu (Total Discount)
                            </th>
                            <td class="u-align-right">
                                <apex:outputText value="{0,number,###,###}">
                                    <apex:param value="{!sumDiscount}" />
                                </apex:outputText>
                            </td>
                        </tr>
                        <tr>
                            <th
                                colspan="9"
                                style="background-color: rgb(218, 218, 218)"
                            >
                                Phiếu Quà Tặng (Voucher)
                            </th>
                            <td class="u-align-right">
                                <apex:variable value="{!0}" var="vouchers" />
                                <!-- <apex:outputPanel rendered="{!NOT(lstVoucher.empty)}"> -->
                                <apex:repeat
                                    value="{!lstVoucher}"
                                    var="voucher"
                                >
                                    <apex:variable
                                        value="{!(vouchers + IF(voucher.Amount__c != null, voucher.Amount__c, 0))}"
                                        var="vouchers"
                                    />
                                    <!-- <apex:outputText value="{0,number,###,###}">
                    <apex:param value="{!voucher.Amount__c}" />
                  </apex:outputText> -->
                                </apex:repeat>
                                <apex:outputText value="{0,number,###,###}">
                                    <apex:param value="{!vouchers}" />
                                </apex:outputText>
                                <!-- </apex:outputPanel> -->
                                <!-- <apex:outputPanel rendered="{!(lstVoucher.empty)}">0</apex:outputPanel> -->
                            </td>
                        </tr>
                        <tr>
                            <th
                                colspan="9"
                                style="background-color: rgb(218, 218, 218)"
                            >
                                Tổng Giá Trị Đơn Hàng (Total Value Amount)
                            </th>
                            <td class="u-align-right">
                                <apex:outputText value="{0,number,###,###}">
                                    <apex:param
                                        value="{!sumSalePrice-paymentAmount}"
                                    />
                                </apex:outputText>
                            </td>
                        </tr>
                    </table>
                </div>
                <apex:outputPanel rendered="{!NOT(lstInstallationItem.empty)}">
                    <div class="row u-m-t-small">
                        <div class="color-yellow u-align-center">SERVICE</div>
                        <table class="u-m-t-xsmall">
                            <colgroup>
                                <col class="short" />
                                <col class="long" />
                                <col class="long" />
                                <col class="short" />
                                <col class="medium" />
                                <col class="medium" />
                                <col class="short" />
                                <col class="medium" />
                                <col class="medium" />
                                <col class="medium" />
                            </colgroup>
                            <tr>
                                <th rowspan="2">STT <br /> (No.)</th>
                                <th rowspan="2">Mã Dịch Vụ <br />(Job Code)</th>
                                <th rowspan="2">
                                    Tên Dịch Vụ <br />(Service Job)
                                </th>
                                <th rowspan="2">ĐVT <br /> (Unit)</th>
                                <th rowspan="2">Giờ Công <br />(Labor hour)</th>
                                <th rowspan="2">Đơn giá <br />(Labour Code)</th>
                                <th rowspan="1" colspan="2">
                                    Giảm Giá/ Dịch Vụ (Discount)
                                </th>
                                <th rowspan="2">
                                    Thành tiền sau giảm giá <br />(Amount after
                                    discount)
                                </th>
                                <th rowspan="2">
                                    Thành tiền bao gồm thuế GTGT <br />(Amount -
                                    VAT)
                                </th>
                            </tr>
                            <tr>
                                <th rowspan="1" colspan="1">%</th>
                                <th rowspan="1" colspan="1">
                                    Số Tiền (Amount)
                                </th>
                            </tr>
                            <apex:variable value="{!1}" var="count2" />
                            <apex:repeat
                                value="{!lstInstallationItem}"
                                var="installation"
                            >
                                <tr>
                                    <td class="u-align-center">
                                        {!FLOOR(count2)}
                                    </td>
                                    <td>{!installation.Product_Code__c}</td>
                                    <td>{!installation.Product2.Name}</td>
                                    <td class="u-align-center">Chiếc</td>
                                    <td class="u-align-center">
                                        {!installation.Product2.Labor_HOUR__c}
                                    </td>
                                    <td class="u-align-right">
                                        <apex:outputText
                                            value="{0,number,###,###}"
                                        >
                                            <apex:param
                                                value="{!IF(installation.Product2.Labor_HOUR__c!=0&&installation.Product2.Labor_HOUR__c!=null,installation.ListPrice/installation.Product2.Labor_HOUR__c,0)}"
                                            />
                                        </apex:outputText>
                                    </td>
                                    <td class="u-align-center">
                                        <apex:outputText value="{0,number,0}">
                                            <apex:param
                                                value="{!IF(installation.Discount__c!=null,installation.Discount__c,0)}"
                                            />
                                        </apex:outputText>
                                    </td>
                                    <td class="u-align-right">
                                        <apex:outputText
                                            value="{0,number,###,###}"
                                        >
                                            <apex:param
                                                value="{!(installation.ListPrice*(IF(installation.Discount__c !=null,installation.Discount__c,0)/100))}"
                                            />
                                        </apex:outputText>
                                    </td>
                                    <td class="u-align-right">
                                        <apex:outputText
                                            value="{0,number,###,###}"
                                        >
                                            <apex:param
                                                value="{!installation.ListPrice-(installation.ListPrice*(IF(installation.Discount__c !=null,installation.Discount__c,0)/100))}"
                                            />
                                        </apex:outputText>
                                    </td>
                                    <td class="u-align-right">
                                        <apex:outputText
                                            value="{0,number,###,###}"
                                        >
                                            <apex:param
                                                value="{!installation.ListPrice}"
                                            />
                                        </apex:outputText>
                                    </td>
                                </tr>
                                <apex:variable
                                    var="count2"
                                    value="{!count2+ 1}"
                                />
                            </apex:repeat>
                            <tr>
                                <th
                                    colspan="9"
                                    style="background-color: rgb(218, 218, 218)"
                                >
                                    Tổng Số Tiền (Total Sale Amount)
                                </th>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!sumInstallationListPrice}"
                                        />
                                    </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <th
                                    colspan="9"
                                    style="background-color: rgb(218, 218, 218)"
                                >
                                    Tổng Giá Trị Chiết Khấu (Total Discount)
                                </th>
                                <td class="u-align-right">
                                    <apex:outputText value="{0,number,###,###}">
                                        <apex:param
                                            value="{!sumInstallationDiscount}"
                                        />
                                    </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <th
                                    colspan="9"
                                    style="background-color: rgb(218, 218, 218)"
                                >
                                    Phiếu Quà Tặng (Voucher)
                                </th>
                                <td class="u-align-right">
                                    <apex:outputPanel
                                        rendered="{!NOT(lstInstallationVoucher.empty)}"
                                    >
                                        <apex:repeat
                                            value="{!lstInstallationVoucher}"
                                            var="voucher"
                                        >
                                            <apex:outputText
                                                value="{0,number,###,###}"
