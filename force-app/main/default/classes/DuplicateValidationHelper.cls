public with sharing class DuplicateValidationHelper {
    public static Duplicate_Record__c createDuplicatedRecord(Lead sourceLead, Lead duplicatedLead){                
        return new Duplicate_Record__c(
            Old_Lead__c =  duplicatedLead.Id,
            New_Lead__c = sourceLead.Id
        );
    }

    public static Duplicate_Record__c createDuplicatedRecordWithAccount(Lead sourceLead, Account duplicatedAccount){        
        return new Duplicate_Record__c(
            New_Lead__c = sourceLead.Id,
            Account__c = duplicatedAccount.Id
        );
    }

    public static void putLeadToExistedPhoneMapping(Map<String, List<Lead>> mapPhoneToExistedLead, Lead newLead){
        if(String.isBlank(newLead.MobilePhone) && String.isBlank(newLead.Phone)){
            return;
        }

        if(!mapPhoneToExistedLead.containsKey(newLead.Phone == Null ? newLead.MobilePhone : newLead.Phone)){
            mapPhoneToExistedLead.put(newLead.Phone == Null ? newLead.MobilePhone : newLead.Phone, new List<Lead> {newLead});
        } else {
            Boolean existed = false;
            for(Lead l : mapPhoneToExistedLead.get(newLead.Phone == Null ? newLead.MobilePhone : newLead.Phone)){
                if(l.Id == newLead.Id){
                    existed = true;
                    break;
                }
            }

            if(!existed){
                mapPhoneToExistedLead.get(newLead.Phone == Null ? newLead.MobilePhone : newLead.Phone).add(newLead);
            }                
        }
    }
    
