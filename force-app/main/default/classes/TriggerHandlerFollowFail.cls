public class TriggerHandlerFollowFail {
    public static void onAfterUpdate(Map<Id, Follow_Fail__c> mapOlds, Map<Id, Follow_Fail__c> mapNews) {
        Set<Id> setIdLead = new Set<Id>();
        for(Follow_Fail__c item : mapNews.values()) {
            System.debug('mapOlds.get(item.Id).OwnerId:'+ mapOlds.get(item.Id).OwnerId);
            System.debug('item.OwnerId:'+ item.OwnerId);
            System.debug('item.Lead__c:'+item.Lead__c);
            
            if(mapOlds.get(item.Id).OwnerId != item.OwnerId && item.RecordTypeId == RecordTypeHelper.FOLLOW_LEAD && item.Lead__c != Null) {
                System.debug('item.Lead__c:'+ item.Lead__c);
                setIdLead.add(item.Lead__c);
            }
        }
        if(!setIdLead.isEmpty()) {
            progressUpdateLead(setIdLead);
        }
    }
    
    private static void progressUpdateLead(Set<Id> setIdLead) {
        try {
            List<Lead> lstLeadQuerry = getLstLeadById(setIdLead);
            System.debug('lstLeadQuerry:'+ lstLeadQuerry);
            if(!lstLeadQuerry.isEmpty()) {
                for(Lead item : lstLeadQuerry) {
                	item.Is_Close__c = false;
            	}
                System.debug('lstLeadQuerry Update:'+ lstLeadQuerry);
                update lstLeadQuerry;
			}
        } catch(exception ex) {
            System.debug('ex get messager errro:' + ex.getMessage() + ' ' + ex.getLineNumber());
        }
    }
    
    private static List<Lead> getLstLeadById(Set<Id> setId) {
        return new List<Lead>([SELECT Id, Is_Close__c FROM Lead WHERE Id IN: setId AND Is_Close__c =: true]);
    }
    
