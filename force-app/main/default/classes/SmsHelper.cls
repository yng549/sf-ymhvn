    public static void sendSingleSms(String historyId, Map<String, String> customDataMapping){
        SMS_History__c[] smsItems = [
            SELECT 
                Id, Error_Code__c, From_Brand_Name__c, Message__c,
                Record_Id__c, Reference_Id__c, Send_Time__c, Template__c,
                To_Phone__c, Type__c, Error_Message__c, OwnerId, Use_Unicode__c,
                Scheduled_Send_Time__c, RecordTypeId
            FROM SMS_History__c
            WHERE Id = :historyId
        ];

        if(smsItems.isEmpty()){
            // Invalid sms history id
            return;
        }

        sendMultipleSms(smsItems, new List<Map<String, String>>{customDataMapping});
    }

    public static List<SMS_History__c> parseSmsHistoriesData(List<SMS_History__c> smsItems, List<Map<String, String>> listCustomDataMapping){
        Map<Id, List<SMS_History__c>> mapTemplateToSmsItem = new Map<Id, List<SMS_History__c>>();
        Map<Id, List<Map<String, String>>> mapTemplateToCustomData = new Map<Id, List<Map<String, String>>>();
        Map<String, SmsClass.SmsTemplateResult> mapSmsTemplateResult = new Map<String, SmsClass.SmsTemplateResult>();

        List<SMS_History__c> parsedSmsItems = new List<SMS_History__c>();

        for(SMS_History__c smsItem : smsItems){
            smsItem.Send_Time__c = Datetime.now();

            // SMS item must have a template associated with it or a combination of message and phone number
            if(String.isBlank(smsItem.Template__c) && (String.isBlank(smsItem.Message__c) || String.isBlank(smsItem.To_Phone__c))){
                smsItem.Error_Code__c = SmsErrorCode.SALESFORCE_EXCEPTION;
                smsItem.Error_Message__c = 'SMS item must have a template lookup or a combination of message and phone number to send out';
            }

            if(String.isBlank(smsItem.Error_Code__c)){
                if(String.isNotBlank(smsItem.Template__c) && String.isBlank(smsItem.Message__c)) {     
                    // Group all sms item that have the same template together so we can parse it in group later
                    if(!mapTemplateToSmsItem.containsKey(smsItem.Template__c)){
                        mapTemplateToSmsItem.put(smsItem.Template__c, new List<SMS_History__c>());
                        mapTemplateToCustomData.put(smsItem.Template__c, new List<Map<String, String>>());
                    }
                    mapTemplateToSmsItem.get(smsItem.Template__c).add(smsItem);
                    mapTemplateToCustomData.get(smsItem.Template__c).add(listCustomDataMapping.get(smsItems.indexOf(smsItem)));
                } else {
                    // If this sms item already have a message then try to convert it to non unicode if required to
                    if(smsItem.Use_Unicode__c == '0'){
                        smsItem.Message__c = StringHelper.convertUnicodeToNonUnicode(smsItem.Message__c);                        
                    }
                    parsedSmsItems.add(smsItem);
                }
            } else {
                // Sms item that contains error will stop processing here
                parsedSmsItems.add(smsItem);
            }
        }

        Map<Id, SMS_Brandname_Template__c> mapTemplates = new Map<Id, SMS_Brandname_Template__c>([
            SELECT 
                Id, Description__c, Object_Name__c, Phone_Field__c, Template__c, Use_Unicode__c
            FROM SMS_Brandname_Template__c
            WHERE Id IN :mapTemplateToSmsItem.keySet()
        ]);

        for(Id templateId : mapTemplateToSmsItem.keySet()){
            if(!mapTemplates.containsKey(templateId)){
                continue;
            }
            
            SMS_Brandname_Template__c template = mapTemplates.get(templateId);
            List<SMS_History__c> listSmsItem = mapTemplateToSmsItem.get(templateId);
            List<SmsClass.SmsTemplateResult> listTemplateResult = SmsTemplateHelper.parseMultipleRecordSmsTemplate(template, listSmsItem, null, mapTemplateToCustomData.get(templateId));

            for(SMS_History__c smsItem : listSmsItem){
                try{
                    Integer index = listSmsItem.indexOf(smsItem);
                    SmsClass.SmsTemplateResult templateResult = listTemplateResult.get(index);

                    if(templateResult != null){
                        List<String> errors = new List<String>();
                        if(String.isBlank(templateResult.message)){
                            errors.add('Empty template message');
                        } else {                    
                            smsItem.Message__c = templateResult.message;
                        }

                        if(String.isBlank(templateResult.phoneNumber)){
                            errors.add('Empty phone number');
                        } else {
                            smsItem.To_Phone__c = templateResult.phoneNumber;
                        }

                        if(String.isNotBlank(templateResult.error)){
                            errors.add(templateResult.error);
                        }

                        if(!errors.isEmpty()){
                            smsItem.Error_Code__c = SmsErrorCode.SALESFORCE_EXCEPTION;
                            smsItem.Error_Message__c = trimErrorMessage(String.join(errors, '. '));
                        }
                    } else {
                        smsItem.Error_Code__c = SmsErrorCode.SALESFORCE_EXCEPTION;
                        smsItem.Error_Message__c = 'Failed to parse template';
                    }
                } catch(Exception e){
                    smsItem.Error_Code__c = SmsErrorCode.SALESFORCE_EXCEPTION;
                    smsItem.Error_Message__c = trimErrorMessage(e.getMessage());
                }

                parsedSmsItems.add(smsItem);
            }
        }

        return parsedSmsItems;
    }

    public static void sendMultipleSms(List<SMS_History__c> smsItems, List<Map<String, String>> listCustomDataMapping){
        if(smsItems.isEmpty()){
            // Invalid sms history id
            return;
        }

        List<SMS_History__c> parsedSmsItems = parseSmsHistoriesData(smsItems, listCustomDataMapping);
        List<SMS_History__c> validSmsItems = new List<SMS_History__c>();
        for(SMS_History__c smsItem : parsedSmsItems){
            smsItem.Queue_For_SMS__c = false;
            if(String.isNotBlank(smsItem.Error_Code__c)){
                continue;
            }                                

            validSmsItems.add(smsItem);
        }

        if(!validSmsItems.isEmpty()){
            callSendSmsAPI(validSmsItems);            
        }

        update parsedSmsItems;
    }
    
