    public static void callSendListSMSVNPT(Id templateId, Id recordId, string phone) {
        SmsClass.SMSDTO smsRequest = SmsTemplateHelper.parseParamSMSContent(templateId, recordId);
        List<SmsClass.ContentParam> params = new List<SmsClass.ContentParam>();
        Map<string, Object> mapData = new Map<string, Object>();
        for(integer i : smsRequest.mapIndexParams.keyset()) {
            string keyparam = smsRequest.mapIndexParams.get(i);
            params.add(new SmsClass.ContentParam(string.valueOf(i), smsRequest.mapFieldParams.get(keyparam)));
        }
        
        string dateNow = Datetime.now().format('dd-MM-yyyy HH:mm a').replace(' PM', '').replace(' AM','');
        mapData.put('name', smsVNPTSetting.Endpoint__c);
        mapData.put('REQID', recordId);
        mapData.put('LABELID', smsRequest.LABELID);
        mapData.put('MOBILELIST', phone.replace('+', ''));
        mapData.put('SCHEDULETIME', dateNow);
        mapData.put('PARAMS', params);
        mapData.put('DATACODING', '0');
        mapData.put('CONTRACTTYPEID', '1');
        mapData.put('TEMPLATEID', smsRequest.TEMPLATEDID);
        mapData.put('CONTRACTID', smsVNPTSetting.CONTRACTID__c);
        mapData.put('AGENTID', smsVNPTSetting.AGENTID__c);
        mapData.put('APIUSER', 'REVZONE');
        mapData.put('APIPASS', 'aBc123456');
        mapData.put('USERNAME', smsVNPTSetting.USERNAME__c);
        mapData.put('ISTELCOSUB', '0');
        string jsondata = JSON.serialize(new SmsClass.SendSMSList(mapData));
        
        HttpRequest req = new HttpRequest();
        req.setEndpoint('http://113.185.0.35:8888/smsbn/api');
        req.setTimeout(120000);
        req.setMethod('POST');    
        req.setBody(jsondata);
        req.setHeader('Content-Type', 'application/json;charset=UTF-8');
        Http h = new Http();
        SMS_History__c smsItem = new SMS_History__c();
        try{
            HttpResponse res = h.send(req);
            if (res != null && res.getStatusCode() == 200) {
                SmsClass.SmsTemplateVNPTResult apiResponse = (SmsClass.SmsTemplateVNPTResult)JSON.deserialize(res.getBody(), SmsClass.SmsTemplateVNPTResult.class);
                // Update sms item when exception arise
                logSMSHistory(recordId, jsondata, apiResponse.RPLY.ERROR, apiResponse.RPLY.ERROR_DESC);
                
