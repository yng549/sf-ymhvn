    //Get Status Inventory Status 
    @AuraEnabled
    public static Case getAssetByCaseId(String strCaseId) {
        return  [SELECT Id, Asset.Inventory_Status__c FROM Case WHERE Id =: strCaseId];
    }

    //Update Status Inventory Status 
    @AuraEnabled 
    public static void updateCase(Case objCase){
        System.debug('objCase :' + objCase.Asset.Inventory_Status__c);
        try{
            Case objCaseUpdate = [SELECT Id, AssetId  FROM Case WHERE Id =: objCase.Id];
            Asset objAsset = [SELECT  Id, Inventory_Status__c FROM Asset WHERE Id =: objCaseUpdate.AssetId];
            objAsset.Inventory_Status__c = objCase.Asset.Inventory_Status__c;
            update objAsset; 
             
        }catch(Exception e){
            AuraHandledException auraEx = new AuraHandledException(e.getMessage());
            auraEx.setMessage(e.getMessage());
            throw auraEx;
        }
    }

    // Get piclist Inventory status
    @AuraEnabled 
    public static Map<String, String> getIndustryFieldValue(){
        Map<String, String> options = new Map<String, String>();
        
        Schema.DescribeFieldResult fieldResult = Asset.Inventory_Status__c.getDescribe();
        
        List<Schema.PicklistEntry> pValues = fieldResult.getPicklistValues();
        for (Schema.PicklistEntry p : pValues) {
            
            options.put(p.getValue(), p.getLabel());
        }
        return options;
    }
    
