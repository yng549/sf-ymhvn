public without sharing class UpdateOwnerLeadSaleSupForSaleSenior {
    @AuraEnabled(Cacheable=true)
    public static List <Lead> getContactsByStatus(String strStatus) {
        User userCurrent = [SELECT Id, AccountId FROM User WHERE ID =: userInfo.getUserId()];
        List<Lead> leadList = [ SELECT Id
                                    , Name
                                    , CreatedDate
                                    , LeadSource
                                    , OwnerId
                                FROM Lead
                                WHERE Owner.Profile.Name =: 'Community DO DL Manager'
                               		AND DO_DL_Create__c =: userCurrent.AccountId
                               		AND Status =: strStatus
                                ORDER BY CreatedDate DESC];
        System.debug('leadList :' + leadList.size());
        return leadList;
    }

    @AuraEnabled
    public static ApiResponse updateOwnerLead(string strLeadId) {
        Savepoint sp = Database.setSavepoint();
        ApiResponse apiResponse = new ApiResponse();
        try {
            
            Lead objLeadById = [SELECT Id, Owner.Name, OwnerId, Owner.Profile.Name, Run_Reassign__c, Is_Round_Robin__c  FROM Lead WHERE Id =: strLeadId];
            if(objLeadById.Is_Round_Robin__c) {
                apiResponse.error = 'Record is updating by orther user.';
                return apiResponse;
            }
            if(String.isNotBlank(objLeadById.Id) && objLeadById.Owner.Profile.Name !=  'Community DO DL Manager') {
                apiResponse.error = 'Owner of lead was transferred for another user before you. The Owner Curent is ' + objLeadById.Owner.Name;
                return apiResponse;
            }
            
            
            //objLeadById.OwnerId = UserInfo.getUserId();
            objLeadById.From_Get_Lead__c = true;
            objLeadById.Is_Round_Robin__c = true;
            objLeadById.Run_Reassign__c = true;
            //UserInfo.getUserId()
            objLeadById.Sale_Sub_DO__c  = UserInfo.getUserId();
            update objLeadById;
			System.debug('objLeadById: ' + objLeadById);
            //processRecords(objLeadById.Id);
            if(apiResponse.error == Null) {
                apiResponse.success = true;
            }
            return apiResponse;
        } catch(Exception ex) {
            apiResponse.error = ex.getMessage() + ' ' + ex.getLineNumber();
            Database.rollback(sp);
            return  apiResponse;
        }
    }
    
    @future
    private static void processRecords(Id objLeadById){   
        // Get those records based on the IDs
        Lead objLead = [SELECT Id, Owner.Name, OwnerId, Owner.Profile.Name, Run_Reassign__c, Is_Round_Robin__c  FROM Lead WHERE Id =: objLeadById];
        objLead.Run_Reassign__c = true;
        objLead.Sale_Sub_DO__c  = UserInfo.getUserId();
        update objLead;
    }

