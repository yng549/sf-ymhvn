// Kế thừa class BaseBatchHandler như lần trước a bảo e viet đó
// 
public with sharing class BatchDODLCreate extends BaseBatchHandler{
    private string objectName;
    
    public BatchDODLCreate() {
        this.objectName = 'Opportunity';
    }

    public override Database.querylocator start(Database.BatchableContext BC) {
        // Chỉ update field DODL_Create__c e ko cần dùng schema query all field lên
        List<String> fields = new List<String>();
        fields.add('Id');
        fields.add('DODL_Create__c');
        fields.add('OwnerId');
        List<String> query = new List<String>();
        string rs = NULL;
        query.add('SELECT  ' + String.join(fields, ','));
        query.add('FROM ' + objectName);
        query.add('WHERE DODL_Create__c =: rs');
        if (Test.isRunningTest()){
            query.add('LIMIT 200');
        }
        return Database.getQueryLocator(String.join(query, ' '));
    }

    private static Map<Id, User> getUser(Set<Id> setIdUser){
        return new Map<Id, User>([SELECT Id,Name, AccountId 
                                  FROM User 
                                  WHERE AccountId!=NULL 
                                  AND Id IN: setIdUser]);
    }

    public override List<SObject> processData(List<SObject> records) {

        List<Opportunity> newData = (List<Opportunity>)records;
        Set<Id> setIdUser = new Set<Id>();
        for(Opportunity oppty : newData) {
            // đẩy ownerID vào List
            // Lấy danh sách đó đi Query 
            setIdUser.add(oppty.OwnerId);
        }
        Map<Id, User> mapUserAccountId = getUser(setIdUser);
        for(Opportunity oppty : newData) {
           
            if(mapUserAccountId.containsKey(oppty.OwnerId)){
                oppty.DODL_Create__c = mapUserAccountId.get(oppty.OwnerId).AccountId;
            }
        }
        // Chỗ này sao lại update danh sách User
        // Trong khi e đã gán giá trị cho record Opportunity 
        // Update danh sách record e query đủ điều kiện trong bacth, ko phải update danh sách User
        update newData;
        return newData;
    }

