public without sharing class CheckIMEIByOrderController {
  @AuraEnabled
  public static ApiResponse searchVoucher(String key) {
    ApiResponse apiResponse = new ApiResponse();

    String searchString = '%' + key + '%';
    List<Voucher__c> lstVoucher = filterVoucher(searchString); // List price book entry

    // if (currentUser.Id == null) {
    //   apiResponse.error = 'Please login!';
    //   return apiResponse;
    // }

    List<ObjectDTO> lstShow = new List<ObjectDTO>();
    for (Voucher__c voucher : lstVoucher) {
        ObjectDTO objDTO = new ObjectDTO(voucher);
      lstShow.add(objDTO);
    }
    apiResponse.result = lstShow;
    apiResponse.success = true;
    return apiResponse;
  }

  private static List<Voucher__c> filterVoucher(String key) {
    List<Voucher__c> lstVoucher = new List<Voucher__c>();

    lstVoucher = [
      SELECT
        Id,
        IMEI__c,
        Voucher_Status__c,
        Amount__c,
        Valid_From__c,
        Valid_To__c,
        Order__c,
        Order__r.Order_Number__c,
        Order__r.AccountId,
        Order__r.Account.Name
      FROM Voucher__c
      WHERE
        RecordTypeId = :RecordTypeHelper.VOUCHER_NEW_BIKE
        AND (IMEI__c LIKE :key
        OR Order__r.Order_Number__c LIKE :key
        OR Order__r.Account.Name LIKE :key) 
       
      ORDER BY IMEI__c
      LIMIT 50
    ];
    return lstVoucher;
  }

  public class ObjectDTO {
    @AuraEnabled
    public String Id { get; set; }
    @AuraEnabled
    public String IMEI { get; set; }
    @AuraEnabled
    public String VoucherStatus { get; set; }
    @AuraEnabled
    public Decimal Amount { get; set; }
    @AuraEnabled
    public Date ValidFrom { get; set; }
    @AuraEnabled
    public Date ValidTo { get; set; }
    @AuraEnabled
    public String OrderId { get; set; }
    @AuraEnabled
    public String OrderNumber { get; set; }
    @AuraEnabled
    public String AccountId { get; set; }
    @AuraEnabled
    public String AccountName { get; set; }

    public ObjectDTO(Voucher__c voucher) {
      this.Id = voucher.Id;
      this.IMEI = voucher.IMEI__c;
      this.VoucherStatus = voucher.Voucher_Status__c;
      this.Amount = voucher.Amount__c;
      this.ValidFrom = voucher.Valid_From__c;
      this.ValidTo = voucher.Valid_To__c;
      this.OrderId = voucher.Order__c;
      this.OrderNumber = voucher.Order__r.Order_Number__c;
      this.AccountId = voucher.Order__r.AccountId;
      this.AccountName = voucher.Order__r.Account.Name;
    }
  }
