public class ScheduleLead Implements Schedulable{            
        public void execute(SchedulableContext sc){
            getTask();
        }
        
        public void getTask(){
            Map<Id, User> objAllUser = getAllUser();
            List<Follow_Fail__c> lstFollowFail = new List<Follow_Fail__c>();
            List<Task> lstTaskUpdate = new List<Task>();
            List<Lead> lstLeadQuerry = getLstLead();
            List<Lead> lstLeadUpdate = new List<Lead>();
            try {
                for (Lead item : lstLeadQuerry) {
                    if(!item.Tasks.isEmpty()) {
                        for (Task obj : item.Tasks) {
                            System.debug('obj:'+ obj.Id);
                            if (obj.Status == 'In Processing' 
                                && obj.ActivityDate < System.today()){
                                    item.Is_Close__c = true;
                                    Follow_Fail__c objFollowFail = new Follow_Fail__c();
                                    objFollowFail.RecordTypeId = RecordTypeHelper.FOLLOW_LEAD;
                                    objFollowFail.Lead__c = item.Id;
                                    objFollowFail.Status__c = 'New';
                                    objFollowFail.Consultant__c = item.OwnerId;
                                    //Task Object Update
                                    obj.Status = 'Not Completed';
                                    obj.Is_Close__c = true;
                                    
                                    
                                    System.debug('Flow_Fouls__c:' + item.Flow_Fouls__c);
                                    if (item.Flow_Fouls__c == 0){
                                        //Lead Object Update
                                        item.Flow_Fouls__c = 1;
                                        System.debug('Case 1:');
                                        if(objAllUser.containsKey(item.OwnerId)) {
                                            System.debug('Case 1:');
                                            objFollowFail.OwnerId = String.isNotBlank(objAllUser.get(item.OwnerId).DODL_Manager__c) ? objAllUser.get(item.OwnerId).DODL_Manager__c : item.OwnerId;
                                        }
                                    } else if(item.Flow_Fouls__c == 1) {
                                        item.Flow_Fouls__c = 2;
                                        if(objAllUser.containsKey(item.OwnerId)) {
                                            objFollowFail.OwnerId = gerUserMarketting().Id;
                                        }
                                    }
                                    lstFollowFail.add(objFollowFail);	
                                    lstTaskUpdate.add(obj);
                                    lstLeadUpdate.add(item);
                                }
                        }
                    }
                    
                }
                System.debug('lstFollowFail:' + lstFollowFail);
                System.debug('lstTaskUpdate:' + lstTaskUpdate);
                System.debug('lstLeadUpdate:' + lstLeadUpdate);
                if(!lstFollowFail.isEmpty()) {
                    insert lstFollowFail;
                }
                if (!lstTaskUpdate.isEmpty()){
                    update lstTaskUpdate;	
                }
                if(!lstLeadUpdate.isEmpty()) {
                    update lstLeadUpdate;
                }
                
            } catch(Exception ex) {
                System.debug('ex get message() : ' + ex.getMessage() + ' ' + ex.getLineNumber());
                lstLeadQuerry.get(0).addError(ex.getMessage() + ' ' + ex.getLineNumber());
            }
        }
        
        private static List<Lead> getLstLead(){
            List<Lead> lstLeadQuerry = [SELECT Id
                                        , Status
                                        , OwnerId
                                        , Is_Close__c
                                        , Flow_Fouls__c
                                        , (SELECT Id
                                           , RecordTypeId
                                           , Status
                                           , Is_Close__c
                                           , ActivityDate
                                           FROM Tasks
                                           WHERE Status !=: 'Closed' 
                                           AND Status !=: 'Not Completed'
                                           AND Status !=: 'Completed'
                                           AND Is_Close__c =: false
                                           LIMIT 1)
                                        FROM Lead
                                        WHERE Is_Close__c =: false 
                                        AND Status !=: 'Unqualified'
                                        AND Status !=: 'Converted'
                                        AND Lead_Type__c !=: 'Service'];
            return lstLeadQuerry;
        }
        
        private static Map<Id, User> getAllUser(){
            Map<Id, User> mapGetAllUser = new Map<Id, User>([SELECT Id, DODL_Manager__c,AccountId, ContactId, ProfileId
                                                             FROM User]);
            return mapGetAllUser;
        }
        
        private static Progress_Flow_Details__c getFlowDetail1Day(){
            return [SELECT Id, Name, Activity_Timeout__c
                    FROM Progress_Flow_Details__c
                    WHERE Activity_Timeout__c = :'1 DAY'
                    LIMIT 1];
        }
        
        private static Progress_Flow_Details__c getFlowDetail5Days(){
            return [SELECT Id, Name, Activity_Timeout__c
                    FROM Progress_Flow_Details__c
                    WHERE Activity_Timeout__c = :'5 DAYS'
                    LIMIT 1];
        }
        
        private static User gerUserMarketting(){
            return [Select Id, Name
                    From User
                    Where Profile.Name = 'MKT Manager'];
        }
