    public static void updateProvinceDistrict(List<Lead> leadList){
    
    Map<String,Id> provinceIdMap= new Map<String,Id>();
    Map<String,Id> districtIdMap= new Map<String,Id>();
    
    Set<String> provinceCodeSet = new Set<String>();
    Set<String> districtCodeSet = new Set<String>(); 
    List<Province__c> provinceList = new List<Province__c>();
    List<District__c> districtList = new List<District__c>();

    for(Lead l:leadList){
        if(leadList!=null&&!leadList.isEmpty()){
            provinceCodeSet.add(l.Province_Code__c);
            districtCodeSet.add(l.District_Code__c);
        }
    }
    provinceList = getProvince(provinceCodeSet);
    System.debug('provinceList'+provinceList);
    districtList= getDistrict(districtCodeSet);
    System.debug('districtList'+districtList);
    //List<Lead> leadToUpdate = new List<Lead>();

    if(!leadList.isEmpty()&&leadList!=null){
        for(Province__c p : provinceList){
            provinceIdMap.put(String.valueOf(p.Province_Code__c),id.valueOf(p.id));
        }
        for(District__c d:districtList){
            districtIdMap.put(String.valueOf(d.District_Code__c),Id.valueOf(d.id));
        }
        for(Lead le:leadList){
            if(provinceIdMap.containsKey(le.Province_Code__c)){
                le.Province__c = provinceIdMap.get(le.Province_Code__c);
            }
            if(districtIdMap.containsKey(le.District_Code__c)){
                le.District__c = districtIdMap.get(le.District_Code__c);
            }
            
        }
      
    }
   
      
    }

    private static List<Province__c> getProvince(Set<String> setProvinceCode){
        return [Select Id ,Province_Code__c from Province__c where Province_Code__c =: setProvinceCode];
    }
    private static List<District__c> getDistrict(Set<String> setDistrictCode){
        return [Select Id, District_Code__c from District__c where District_Code__c =: setDistrictCode];
    }
