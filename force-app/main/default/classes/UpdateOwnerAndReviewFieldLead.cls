public with sharing class UpdateOwnerAndReviewFieldLead {
    private static String strProfileName4 = 'Community Sale Profile';
	private final static String SYSTEM_ERROR = 'Lead đã được assign cho User có profile là Community Sale.';

	private static Map<Id, Profile> objProfileSale = new Map<Id, Profile>([SELECT Id, Name
	                                                                       FROM Profile
	                                                                       WHERE Name = :strProfileName4]);

	public UpdateOwnerAndReviewFieldLead() {

    }

    @AuraEnabled
	public static LeadDTO getLeadData(String strId) {
		try {
            String FirstName = UserInfo.getFirstName();
            String LastName = UserInfo.getLastName();
            String UserId = UserInfo.getUserId();
            String strFullName = (FirstName != Null ? FirstName : '') + ' ' + (LastName != Null ? LastName : '');
            LeadDTO objLeadDTO = new LeadDTO(strFullName);
            return objLeadDTO;
		} catch (Exception ex) {
			throw new AuraHandledException(String.format('Loi system', new List<String>{ex.getMessage()}));
		}
	}

    @AuraEnabled
	public static void updateLeadData(String strId) {
		String UserId = UserInfo.getUserId();
		Lead objLead = [SELECT Id, OwnerId, Run_Reassign__c, Owner.ProfileId FROM Lead WHERE Id =: strId];
		if(!objProfileSale.containsKey(objLead.Owner.ProfileId)) {
			objLead.OwnerId = UserId;
			objLead.Run_Reassign__c = true;
            objLead.From_Get_Lead__c = true;
			update objLead;

		} else {
			throw new AuraHandledException(SYSTEM_ERROR);
		}
	}

    public class LeadDTO {

		@AuraEnabled
		public String strOwnerId {get; set;}
		@AuraEnabled
		public Boolean boolChecked {get; set;}

		public LeadDTO() {}

		public LeadDTO(String strFullName) {
			this.boolChecked = true;
			this.strOwnerId = strFullName;
		}
	}
    
